{
  "kind": "collectionType",
  "collectionName": "payment_methods",
  "info": {
    "singularName": "payment-method",
    "pluralName": "payment-methods",
    "displayName": "PaymentMethod"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "casinos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::casino.casino",
      "mappedBy": "paymentMethods"
    }
  }
}
